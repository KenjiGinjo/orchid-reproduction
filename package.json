{
  "name": "orchid-reproduction",
  "version": "0.0.0",
  "scripts": {
    "check-type": "tsc --noEmit",
    "commit": "cz",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint:js": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --cache --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit $1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,md,json}": "prettier --check",
    "**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": "@xstools-dev/prettier-config/base",
  "eslintConfig": {
    "extends": "@xstools-dev/eslint-config/base"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@xstools/utility": "^0.13.2",
    "date-fns": "^3.6.0",
    "orchid-orm": "^1.32.6",
    "orchid-orm-test-factory": "^0.7.14",
    "rake-db": "^2.22.6"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "@types/node": "^20.14.9",
    "@xstools-dev/mix": "^0.3.2",
    "typescript": "^5.5.2"
  },
  "packageManager": "pnpm@9.4.0"
}
